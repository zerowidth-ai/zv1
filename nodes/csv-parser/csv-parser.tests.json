    "inputs": {"csv": "name,age,city\nJohn,30,New York\nJane,25,Boston"},
    "expected": {
      "data": [
        {"name": "John", "age": "30", "city": "New York"},
        {"name": "Jane", "age": "25", "city": "Boston"}
      ],
      "headers": ["name", "age", "city"],
      "error": "",
      "success": true
    }
  },
  {
    "description": "Parse CSV without headers",
    "inputs": {"csv": "John,30,New York\nJane,25,Boston"},
