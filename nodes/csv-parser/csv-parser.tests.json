    "expected": {
      "data": [
        {"name": "John", "age": "30", "city": "New York"},
        {"name": "Jane", "age": "25", "city": "Boston"}
      ],
      "headers": ["name", "age", "city"],
      "error": "",
      "success": true
    }
  },
  {
    "description": "Parse CSV with quoted values",
    "inputs": {"csv": "name,description\nJohn,\"Developer, Senior\"\nJane,\"Manager, IT\""},
