[
  {
    "description": "Always pass through value (0% probability)",
    "inputs": {
      "value": "test string",
      "nullProbability": 0.0
    },
    "expected": "Should always return 'test string'",
    "shouldThrow": false
  },
  {
    "description": "Always return null (100% probability)",
    "inputs": {
      "value": 42,
      "nullProbability": 1.0
    },
    "expected": "Should always return null",
    "shouldThrow": false
  },
  {
    "description": "50% chance of null",
    "inputs": {
      "value": {"key": "value"},
      "nullProbability": 0.5
    },
    "expected": "Should return either the object or null with 50% probability",
    "shouldThrow": false
  },
  {
    "description": "Default probability (10%)",
    "inputs": {
      "value": [1, 2, 3]
    },
    "expected": "Should return the array with 90% probability, null with 10% probability",
    "shouldThrow": false
  },
  {
    "description": "Handle edge case probability values",
    "inputs": {
      "value": "edge case",
      "nullProbability": 1.5
    },
    "expected": "Should clamp probability to 1.0 and always return null",
    "shouldThrow": false
  },
  {
    "description": "Handle negative probability",
    "inputs": {
      "value": "negative test",
      "nullProbability": -0.5
    },
    "expected": "Should clamp probability to 0.0 and always return the value",
    "shouldThrow": false
  },
  {
    "description": "Test with complex data structure",
    "inputs": {
      "value": {
        "nested": {
          "array": [1, 2, 3],
          "string": "hello",
          "number": 123.45
        }
      },
      "nullProbability": 0.3
    },
    "expected": "Should return either the complex object or null with 30% probability",
    "shouldThrow": false
  }
] 