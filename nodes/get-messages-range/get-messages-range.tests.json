[
  {
    "description": "Get messages from index 1 to 3",
    "inputs": {
      "messages": [
        {"role": "user", "content": "Message 0"},
        {"role": "assistant", "content": "Message 1"},
        {"role": "user", "content": "Message 2"},
        {"role": "assistant", "content": "Message 3"},
        {"role": "user", "content": "Message 4"}
      ],
      "start": 1,
      "end": 3
    },
    "expected": {
      "count": 2
    }
  },
  {
    "description": "Get messages from index 0 to end using -1",
    "inputs": {
      "messages": [
        {"role": "user", "content": "Message 0"},
        {"role": "assistant", "content": "Message 1"},
        {"role": "user", "content": "Message 2"}
      ],
      "start": 0,
      "end": -1
    },
    "expected": {
      "count": 3
    }
  },
  {
    "description": "Get messages from index 2 to end using -1",
    "inputs": {
      "messages": [
        {"role": "user", "content": "Message 0"},
        {"role": "assistant", "content": "Message 1"},
        {"role": "user", "content": "Message 2"},
        {"role": "assistant", "content": "Message 3"}
      ],
      "start": 2,
      "end": -1
    },
    "expected": {
      "count": 2
    }
  },
  {
    "description": "Get single message at index 1",
    "inputs": {
      "messages": [
        {"role": "user", "content": "Message 0"},
        {"role": "assistant", "content": "Message 1"},
        {"role": "user", "content": "Message 2"}
      ],
      "start": 1,
      "end": 2
    },
    "expected": {
      "count": 1
    }
  },
  {
    "description": "Handle start index beyond array length",
    "inputs": {
      "messages": [
        {"role": "user", "content": "Message 0"},
        {"role": "assistant", "content": "Message 1"}
      ],
      "start": 5,
      "end": 10
    },
    "expected": {
      "count": 0
    }
  },
  {
    "description": "Handle end index beyond array length",
    "inputs": {
      "messages": [
        {"role": "user", "content": "Message 0"},
        {"role": "assistant", "content": "Message 1"}
      ],
      "start": 0,
      "end": 10
    },
    "expected": {
      "count": 2
    }
  },
  {
    "description": "Handle empty message array",
    "inputs": {
      "messages": [],
      "start": 0,
      "end": 1
    },
    "expected": {
      "count": 0
