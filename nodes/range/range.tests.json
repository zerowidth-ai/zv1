[
  {
    "description": "Basic range",
    "inputs": {
      "numbers": [1, 3, 5, 7, 9]
    },
    "expected": {
      "min": 1,
      "max": 9,
      "range": 8
    }
  },
  {
    "description": "Negative numbers",
    "inputs": {
      "numbers": [-5, -3, 0, 2, 4]
    },
    "expected": {
      "min": -5,
      "max": 4,
      "range": 9
    }
  },
  {
    "description": "All same values",
    "inputs": {
      "numbers": [42, 42, 42]
    },
    "expected": {
      "min": 42,
      "max": 42,
      "range": 0
    }
  },
  {
    "description": "Single value",
    "inputs": {
      "numbers": [7]
    },
    "expected": {
      "min": 7,
      "max": 7,
      "range": 0
    }
  },
  {
    "description": "Empty array",
    "inputs": {
      "numbers": []
    },
    "expected": {
      "min": 0,
      "max": 0,
      "range": 0
    }
  },
  {
    "description": "Mixed valid/invalid values",
    "inputs": {
      "numbers": [1, "2", -3, null, 4, "invalid"]
    },
    "expected": {
      "min": -3,
      "max": 4,
      "range": 7
    }
  }
] 